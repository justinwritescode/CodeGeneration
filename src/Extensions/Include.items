<Project>
  <ItemGroup>
		<Compile Remove="@(Compile)" />
    <PackageFile Include="$(MSBuildThisFileDirectory)**/*.cs" PackagePath="contentFiles/%(RecursiveDir)%(Filename)%(Extension)" />
    <PackageFile Remove="obj/**/*.cs" />
		<SourceFilesProjectOutputGroupOutput Remove="@(SourceFilesProjectOutputGroupOutput)" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.CodeAnalysis.Analyzers" PrivateAssets="all" IncludeAssets="runtime; build; native; contentfiles; analyzers; buildtransitive" />
    <PackageReference Include="Microsoft.CodeAnalysis.CSharp.Workspaces" />
    <PackageReference Include="Microsoft.CodeAnalysis.Common" VersionOverride="[4.3.1,)" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="System.Runtime" />
  </ItemGroup>
</Project>
